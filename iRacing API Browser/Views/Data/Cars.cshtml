@{
    ViewData["Title"] = "Cars Data";
}
@model Aydsko.iRacingData.Cars.CarInfo[]

<h1>@ViewData["Title"]</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Make</th>
            <th scope="col">Model</th>
            <th scope="col">Name</th>
            <th scope="col">CarWeight</th>
            <th scope="col">Hp</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var car in Model)
        {
            <tr>
                <td>@car.CarMake</td>
                <td>@car.CarModel</td>
                <td>@car.CarName</td>
                <td>@car.CarWeight</td>
                <td>@car.Hp</td>
            </tr>
        }
    </tbody>
</table>